          "format": "table",
          "rawQuery": true,
          "rawSql": "SELECT\r\n  e.severity,\r\n  hstgrp.name AS grupo,\r\n  CASE\r\n    WHEN e.severity = 0 THEN 'Not classified'\r\n    WHEN e.severity = 1 THEN 'Information'\r\n    WHEN e.severity = 2 THEN 'Warning'\r\n    WHEN e.severity = 3 THEN 'Average'\r\n    WHEN e.severity = 4 THEN 'High'\r\n    WHEN e.severity = 5 THEN 'Disaster'\r\n  END AS severidade,\r\n  e.clock,\r\n  DAYOFWEEK(FROM_UNIXTIME(e.clock)) dia_semana,\r\n  DAYNAME(FROM_UNIXTIME(e.clock)) dia_semana2,\r\n  FROM_UNIXTIME(e.clock) AS datahora,\r\n  DATE_ADD(FROM_UNIXTIME(e.clock), INTERVAL 3 HOUR) AS datahora_ajustada,\r\n  DAY(FROM_UNIXTIME(e.clock)) AS dia,\r\n  MONTH(FROM_UNIXTIME(e.clock)) AS mes,\r\n  YEAR(FROM_UNIXTIME(e.clock)) AS ano,\r\n  HOUR(FROM_UNIXTIME(e.clock)) AS hora,\r\n  MINUTE(FROM_UNIXTIME(e.clock)) AS minuto,\r\n  e.name AS incidente,\r\n  ht.hostid,\r\n  ht.host,\r\n  ht.name AS hostname,\r\n  GROUP_CONCAT(DISTINCT eth.ip SEPARATOR ' | ') AS ip,\r\n  MAX(e.acknowledged) AS acknowledged,\r\n  COUNT(DISTINCT e.eventid) AS qtde_ack,\r\n  CASE\r\n    WHEN MIN(e2.clock) IS NOT NULL THEN 'RESOLVED'\r\n    ELSE 'NOT RESOLVED'\r\n  END AS r_status,\r\n  CASE\r\n    WHEN MIN(e2.clock) IS NOT NULL THEN (MIN(e2.clock) - MIN(e.clock))\r\n    ELSE NULL\r\n  END AS r_duracao_seg,\r\n  MIN(e2.clock) AS r_clock,\r\n  DAYOFWEEK(FROM_UNIXTIME(MIN(e2.clock))) r_dia_semana,\r\n  DAYNAME(FROM_UNIXTIME(MIN(e2.clock))) r_dia_semana2,\r\n  FROM_UNIXTIME(MIN(e2.clock)) AS r_datahora,\r\n  DATE_ADD(FROM_UNIXTIME(MIN(e2.clock)), INTERVAL 3 HOUR) AS datahorar_ajustada,\r\n  DAY(FROM_UNIXTIME(MIN(e2.clock))) AS r_dia,\r\n  MONTH(FROM_UNIXTIME(MIN(e2.clock))) AS r_mes,\r\n  YEAR(FROM_UNIXTIME(MIN(e2.clock))) AS r_ano,\r\n  HOUR(FROM_UNIXTIME(MIN(e2.clock))) AS r_hora,\r\n  MINUTE(FROM_UNIXTIME(MIN(e2.clock))) AS r_minuto\r\nFROM\r\n  events e\r\n  LEFT JOIN event_recovery er1 ON e.eventid = er1.eventid\r\n  INNER JOIN functions f ON e.objectid = f.triggerid\r\n  INNER JOIN items i ON f.itemid = i.itemid\r\n  INNER JOIN hosts ht ON i.hostid = ht.hostid\r\n  LEFT JOIN events AS e2 ON e2.eventid = er1.r_eventid\r\n  JOIN triggers AS t ON e.objectid = t.triggerid\r\n  JOIN hosts_groups ON ht.hostid = hosts_groups.hostid\r\n  JOIN hstgrp ON hosts_groups.groupid = hstgrp.groupid\r\n  LEFT JOIN interface eth ON eth.hostid = ht.hostid\r\nWHERE\r\n  e.source = '0'\r\n  AND e.object = '0'\r\n  AND NOT EXISTS (\r\n    SELECT NULL\r\n    FROM event_suppress es\r\n    WHERE es.eventid = e.eventid\r\n  )\r\n  AND e.value = 1\r\n  AND e.severity IN (4, 5, 6)\r\n  AND hstgrp.name LIKE '${grupo:csv$}'\r\n  AND $__unixEpochFilter(e.clock)\r\nGROUP BY\r\n  e.eventid, ht.hostid, ht.host, ht.name, hstgrp.name\r\n#HAVING\r\n#  r_status = 'NOT RESOLVED'\r\nORDER BY\r\n  MIN(e.clock) ASC;\r\n",
