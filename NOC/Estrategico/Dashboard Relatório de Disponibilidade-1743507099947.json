          "rawQuery": true,
          "rawSql": "SELECT COUNT(*) AS 'Disponibilidade 90% a 100%'\r\nFROM (\r\n    SELECT a.*,\r\n        (SELECT GROUP_CONCAT(DISTINCT eth.ip SEPARATOR ' | ') \r\n         FROM interface eth \r\n         WHERE eth.hostid = a.hostid \r\n         ORDER BY ip ASC) AS ip,\r\n        (SELECT (sla_avg * 100) \r\n         FROM (\r\n             SELECT AVG(value_avg) AS sla_avg\r\n             FROM trends\r\n             WHERE itemid = a.itemid\r\n               AND $__unixEpochFilter(clock)\r\n\r\n             UNION ALL\r\n\r\n             SELECT AVG(value_avg) AS sla_avg\r\n             FROM trends_uint\r\n             WHERE itemid = a.itemid\r\n               AND $__unixEpochFilter(clock)\r\n         ) AS t\r\n         WHERE sla_avg IS NOT NULL\r\n        ) AS sla_disponibilidade\r\n    FROM (\r\n        SELECT i.itemid, h.hostid, h.name, h.host \r\n        FROM hosts AS h \r\n        INNER JOIN items AS i ON h.hostid = i.hostid\r\n        WHERE i.key_ = 'icmpping'\r\n\r\n            -- somente hosts e items ativos\r\n            AND h.status = '0'\r\n            AND i.status = '0'\r\n\r\n            -- grupo \r\n            AND h.hostid IN (\r\n                SELECT DISTINCT hostid\r\n                FROM hosts_groups h\r\n                INNER JOIN hstgrp h2 ON h.groupid = h2.groupid\r\n                WHERE h2.NAME IN ('${grupo:csv}')\r\n            )\r\n    ) AS a\r\n) AS resultado_final\r\nWHERE sla_disponibilidade > 90;\r\n",
