          "rawQuery": true,
          "rawSql": "SELECT a.*\r\n    ,(SELECT GROUP_CONCAT(distinct eth.ip SEPARATOR ' | ') \r\n      FROM interface eth \r\n      WHERE eth.hostid = a.hostid \r\n      ORDER BY ip ASC) AS ip\r\n    ,(SELECT sla_avg * 100 \r\n      FROM (\r\n          SELECT avg(value_avg) as sla_avg\r\n          FROM trends\r\n          WHERE itemid = a.itemid\r\n            AND $__unixEpochFilter(clock)\r\n    \r\n          UNION ALL\r\n\r\n          SELECT avg(value_avg) as sla_avg\r\n          FROM trends_uint\r\n          WHERE itemid = a.itemid\r\n            AND $__unixEpochFilter(clock)\r\n      ) AS t\r\n      WHERE sla_avg IS NOT NULL\r\n    ) AS sla_media\r\n    ,(100 - (SELECT sla_avg * 100 \r\n      FROM (\r\n          SELECT avg(value_avg) as sla_avg\r\n          FROM trends\r\n          WHERE itemid = a.itemid\r\n            AND $__unixEpochFilter(clock)\r\n    \r\n          UNION ALL\r\n\r\n          SELECT avg(value_avg) as sla_avg\r\n          FROM trends_uint\r\n          WHERE itemid = a.itemid\r\n            AND $__unixEpochFilter(clock)\r\n      ) AS t\r\n      WHERE sla_avg IS NOT NULL\r\n    )) AS indisponibilidade_media\r\nFROM (\r\n    SELECT i.itemid, h.hostid, h.name, h.host \r\n    FROM hosts AS h \r\n    INNER JOIN items AS i ON h.hostid = i.hostid\r\n    WHERE i.key_ = 'icmpping'\r\n    \r\n        -- somente hosts e items ativos\r\n        AND h.status = '0'\r\n        AND i.status = '0'\r\n\r\n        -- grupo \r\n        AND h.hostid IN (\r\n            SELECT distinct hostid\r\n            FROM hosts_groups h, hstgrp h2\r\n            WHERE h.groupid = h2.groupid\r\n            AND h2.NAME IN ('${grupo:csv}')\r\n        )\r\n) AS a\r\nHAVING indisponibilidade_media IS NOT NULL\r\n   AND indisponibilidade_media BETWEEN 10 AND 90\r\nORDER BY indisponibilidade_media DESC\r\nLIMIT 10;\r\n",
