}\n\n@media screen and (max-width: 768px) {\n    .logo-text {\n        display: none; /* Ocultar o texto em dispositivos menores */\n    }\n}\n\n\n  </style>\n\n<!DOCTYPE html>\n<html lang=\"en\">\n\n<head>\n    <meta charset=\"UTF-8\">\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\n    <title>Header Responsivo</title>\n     <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css\" integrity=\"sha384-+ccJqDOB65iT8rOiVYDrC5bM5T9eyGrBfMjQrKDO5wkeDTcg9q7lsTz2tx3bo2R5\" crossorigin=\"anonymous\">\n    <!-- Incluindo Font Awesome -->\n    <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.0.0-beta3/css/all.min.css\" integrity=\"sha384-+ccJqDOB65iT8rOiVYDrC5bM5T9eyGrBfMjQrKDO5wkeDTcg9q7lsTz2tx3bo2R5\" crossorigin=\"anonymous\">\n</head>\n<body>\n    <header class=\"custom-header\">\n        <div class=\"custom-logo\">\n           \n         <a href=\"#\"><img src=\"\thttps://www.lgti.com.br/wp-content/uploads/2021/10/01_Si%CC%81mbolo_LGTI.png\" alt=\"Logo\"></a>\n          <span class=\"logo-text\">Relatório de Disponibilidade</span>\n        </div>\n        <nav>\n            <ul class=\"custom-nav-links\">\n                <li><a href=\"https://portal.cloudash.com.br/d/dashboard-relatorio/dashboard-relatorio?orgId=106&refresh=2h\"><i class=\"fa fa-file-text-o\"></i> Relatório</a></li>\n                <li><a href=\"https://portal.cloudash.com.br/d/dashboard-ativos/dashboard-ativos?orgId=106&refresh=15m\"><i class=\"fa fa-info-circle\"></i> Informações Ativos</a></li>\n                <li><a href=\"https://portal.cloudash.com.br/d/dashboard-manutencao/dashboard-manutencao?orgId=106&refresh=5m\"><i class=\"fa fa-calendar\"></i> Janelas de Manutenção</a></li>\n               \n               \n            </ul>\n        </nav>\n    </header>\n    <script src=\"script.js\"></script>\n</body>\n</html>\n<script>\n    const navSlide = () => {\n    const burger = document.querySelector('.burger');\n    const nav = document.querySelector('.custom-nav-links');\n\n    burger.addEventListener('click', () => {\n        nav.classList.toggle('show');\n    });\n}\n\nnavSlide();\n</script>",
